t = int(input())
while t:
    n, x = map(int, input().split())
    a = list(map(int, input().split()))
    m = 0
    for i in range(n):
        if i == 0:
            d = a[i] - 0

        else:
            d = a[i] - a[i - 1]
        m = max(m, d)
    m = max(m, 2 * (x - a[-1]))

    print(m)
    t -= 1
