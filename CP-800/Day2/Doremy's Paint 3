import collections

t = int(input())
while t:
    n = int(input())
    a = list(map(int, input().split()))
    d = collections.defaultdict(int)
    for i in range(n):
        d[a[i]] += 1
    if len(d) > 2:
        print("No")
    elif len(d) == 1:
        print("yes")
    else:
        keys = list(d.values())
        if n % 2 == 1 and abs(keys[0] - keys[1]) == 1:
            print("Yes")
        elif keys[0] == keys[1]:
            print("yes")
        else:
            print("no")
    t -= 1
