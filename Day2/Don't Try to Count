t = int(input())
while t:
    n, m = map(int, input().split())
    x = input()
    s = input()
    ans = 0
    f = 0

    while ans < 6:
        if x.find(s) != -1:
            f = 1
            break

        x += x
        ans += 1
    if f == 1:
        print(ans)
    else:
        print(-1)
    t -= 1
